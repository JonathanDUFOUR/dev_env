default partial xkb_keycodes "axis" {
  // The X11 protocol reserves the first 8 values for special purposes
  minimum = 8;
  maximum = 255;

  include "acer_predator_helios_300(1st_row_from_top)"
  include "acer_predator_helios_300(2nd_row_from_top)"
  include "acer_predator_helios_300(3rd_row_from_top)"
  include "acer_predator_helios_300(4th_row_from_top)"
  include "acer_predator_helios_300(5th_row_from_top)"
  include "acer_predator_helios_300(6th_row_from_top)"
  include "acer_predator_helios_300(fn_key_combinations)"

  // Finally, those are the special keys that CANNOT be mapped using the XKB protocol
  // since they are out of the hard bounds of the protocol (greater than 255)
  // <PAD0> = 540; // Touchpad off
  // <PAD1> = 539; // Touchpad on
};

hidden partial xkb_keycodes "1st_row_from_top" {
  <A00> = 9;
