hidden partial xkb_keycodes "fn_key_combinations" {
  <SLP>  = 150; // Sleep
  <VMOD> = 235; // Switch video mode
  <SCRL> = 78;  // Scroll Lock
  <INS>  = 118; // Insert
  <NEXT> = 171; // Next track
  <PLAY> = 172; // Play/Pause
  <PREV> = 173; // Previous track
  <STOP> = 174; // Stop
  <MUTE> = 121; // Mute
  <VOL-> = 122; // Volume Down
  <VOL+> = 123; // Volume Up
  <BRT-> = 232; // Brightness Down
  <BRT+> = 233; // Brightness Up

  // <SCSW> = ???; // Screen switch
};
